{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "better-docs": {
      "name": "Joyjet application test Documentation",
      "logo": "img/logo.gif",
      "hideGenerator": false,
      "navigation": [
        {
          "label": "Github",
          "href": "https://github.com/danprates/joyjet-backend-test"
        }
      ]
    }
  },
  "opts": {
    "template": "node_modules/better-docs",
    "recurse": true,
    "destination": "./docs/"
  }
}
